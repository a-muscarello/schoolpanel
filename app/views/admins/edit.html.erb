

<%= form_for @admin do |f| %>  
    <%= f.label :user_name %>  
    <%= f.text_field :user_name, value: @admin.user_name%>  
    <%= f.label :password %>  
    <%= f.text_field :password, value: @admin.password%>  
    <%= f.submit %>
<% end %>


<%= link_to 'Delete', admin_path(@admin), method: :delete %> 