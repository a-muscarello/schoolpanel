
<table class="table table-striped">
    <thead class="thead-dark">
      <tr>
      <%= form_for @student do |f| %>
        <td><%= @student.first_name %></td>
        <td><%= @student.last_name %></td>
        <td><%= @student.age %></td>
        <td><%= @student.highest_education %></td>
        @student.cohort_id %></td>
        <%= f.submit %>
      <% end %>
      </tr>
    </thead>
</table>
