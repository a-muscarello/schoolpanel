
<table class="table table-striped">
  <thead class="thead-dark">
    <tr>
    <%= form_for @student do |f| %>
      <td>First Name: <%= @student.first_name %></td>
      <td>Last Name: <%= @student.last_name %></td>
      <td>Age: <%= @student.age %></td>
      <td>Highest Education: <%= @student.highest_education %></td>
      <td>Cohort id: <%= @student.cohort_id %></td>
      <%= f.submit %>
      <% end %>
    </tr>
  </thead>
</table>
