

<% @cohort.each do |cohort| %>
    <div>
        <h3>Cohort Name: <%= cohort.name %></h3>
        <h3>Start Date: <%= cohort.start_date %></h3>
        <h3>End Date: <%= cohort.end_date %></h3>
        <h3>Instructor Name: <%= cohort.instructor_name %></h3>
        <h3>Course ID: <%= cohort.course_id %></h3>
    </div>
<% end %>
