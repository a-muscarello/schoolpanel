

<%= form_for @instructor do |f| %>
    <%= f.label :first_name %>
    <%= f.text_field :first_name, value: @instructor.first_name %>
    <%= f.label :last_name %>
    <%= f.text_field :last_name, value: @instructor.last_name %>
    <%= f.label :age %>
    <%= f.number_field :age, value: @instructor.age %>
    <%= f.label :salary %>
    <%= f.number_field :salary, value: @instructor.salary %>
    <%= f.label :highest_education %>
    <%= f.text_field :highest_education, value: @instructor.highest_education %>
    <%= f.submit %>
<% end %>


<%= link_to 'Delete', instructor_path(@instructor), method: :delete %>