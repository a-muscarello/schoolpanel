
<table class="table table-striped">
  <thead class="thead-dark">
    <tr>
    <form>
      <th scope="col">First Name:</th>
      <th scope="col">Last Name</th>
      <th scope="col">Age</th>
      <th scope="col">Highest Education</th>
      <th scope="col">Cohort  ID</th>
    </form>
    </tr>
  </thead>

  <tbody>
    <tr>
      <form>
        <% @student.each do |student| %>
            <td><%= student.first_name %></td>
            <td><a class="nav-link" href="/students"><%= student.last_name %></a></td>
            <td><%= student.age %></td>
            <td><%= student.highest_education %></td>
            <td><%= student.cohort_id %></td> 
            </tr> 
        <% end %>
      </form>
  </tbody>
</table>
