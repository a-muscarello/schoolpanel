
<table class="table table-striped">
<thead class="thead-dark">
  <tr>
    <form>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Age</th>
      <th scope="col">Salary</th>
      <th scope="col">Highest Education</th>
      <th scope="col">Cohort ID</th>
    </form>
  </tr>
</thead>

<tbody>
  <tr>
    <form>
    <% @instructor.each do |instructor| %>
        <td><%= instructor.first_name %></td>
        <td><a class="nav-link" href="/instructors/<%= instructor.id %>"><%= instructor.last_name %></a></td>
        <td><%= instructor.age %></td>
        <td><%= instructor.salary %></td>
        <td><%= instructor.highest_education %></td>
        <td><%= instructor.cohort_id %></td>  

        <td><%= link_to "Edit", edit_instructor_path(instructor) %></td>
        <td id="instructor-<%= instructor.id %>"><%= link_to "Delete", instructor_path(instructor), remote: true, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
        <% end %>
    </form>
</tbody>
</table>

