
        <%= form_for @instructor do |f| %>
      
            <ul>
            <li><%= f.label :first_name %></li>
            <li><%= f.text_field :first_name, value: @instructor.first_name %></li>
    
            <li><%= f.label :last_name %></li>
            <li><%= f.text_field :last_name, value: @instructor.last_name %></li>
    
            <li><%= f.label :age %></li>
            <li><%= f.number_field :age, value: @instructor.age %></li>
    
            <li><%= f.label :salary %></li>
            <li><%= f.number_field :salary, value: @instructor.salary %></li>
    
            <li><%= f.label :highest_education %></li>
            <li><%= f.text_field :highest_education, value: @instructor.highest_education %></li>
            
            <%= f.submit %>
            <% end %>
            </ul>
        
    <%= link_to 'Delete', instructor_path(@instructor), remote: true, method: :delete, data: { confirm: 'Are you sure?' } %>