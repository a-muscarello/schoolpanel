
<%= form_for @instructor do |f| %>
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
    <%= f.label :age %>
    <%= f.number_field :age %>
    <%= f.label :salary %>
    <%= f.number_field :salary %>
    <%= f.label :highest_education %>
    <%= f.text_field :highest_education %>
    <%= f.submit %>
<% end %>
   