


<%= form_for @cohort do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name, value: @cohort.name %>
    <%= f.label :start_date %>
    <%= f.text_field :start_date, value: @cohort.start_date %>
    <%= f.label :end_date %>
    <%= f.text_field :end_date, value: @cohort.end_date %>
    <%= f.label :instructor_name %>
    <%= f.text_field :instructor_name, value: @cohort.instructor_name %>
    <%= f.label :course_id %>
    <%= f.number_field :course_id, value: @cohort.course_id %>
    <%= f.submit %>
<% end %>


<%= link_to 'Delete', cohort_path(@cohort), method: :delete %>