

<%= form_for @student do |f| %>
    <%= f.label :first_name %>
    <%= f.text_field :first_name, value: @student.first_name %>
    <%= f.label :last_name %>
    <%= f.text_field :last_name, value: @student.last_name %>
    <%= f.label :age %>
    <%= f.number_field :age, value: @student.age %>
    <%= f.label :highest_education %>
    <%= f.text_field :highest_education, value: @student.highest_education %>
    <%= f.submit %>
<% end %>


<%= link_to 'Delete', student_path(@student), method: :delete %>